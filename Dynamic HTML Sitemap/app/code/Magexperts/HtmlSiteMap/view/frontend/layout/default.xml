        <referenceBlock name="footer_links">            
            <block class="Magexperts\HtmlSiteMap\Block\ItemsCollection" name="custom_block" as="custom_block" template="Magexperts_HtmlSiteMap::custom.phtml" after="-"/>
        </referenceBlock>
    </body>
</page>
